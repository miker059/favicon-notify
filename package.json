{
  "name": "favicon-notify",
  "version": "0.1.2",
  "description": "Plugin for displaying notifications on the favicon of the site",
  "keywords": [
    "favicon",
    "favicon-notify",
    "favicon-notification",
    "favico",
    "favico.js"
  ],
  "author": "miker059",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/miker059/favicon-notify.git"
  },
  "homepage": "https://github.com/miker059/favicon-notify#readme",
  "bugs": {
    "url": "https://github.com/miker059/favicon-notify/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "dev": "rollup -w -c rollup.dev.config.ts",
    "build": "rollup -c rollup.config.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-html": "^0.2.4",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/jest": "^27.0.3",
    "jest": "^27.4.5",
    "prettier": "^2.5.1",
    "rollup": "^2.61.1",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^27.1.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.5.4"
  },
  "files": [
    "dist"
  ]
}
